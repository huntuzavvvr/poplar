#include <iostream>
using namespace std;

int main()
{
    int k, m, x, y;
    cin >> k >> m >> x >> y;
    int count1,count2;
    count1 = 0;
    count2 = 0;
    for (int i=x; i <= y; i++) {
        if (i % k==0) {
            count1++;
        }
        if (i % m==0) {
            count2++;
        }
    }
    cout << count1 - count2;
    return 0;
}
